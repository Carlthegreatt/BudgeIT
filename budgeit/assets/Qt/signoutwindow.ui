                   <property name="styleSheet">
                    <string notr="true">/* Base style for QToolButton */
QToolButton {

    
   background: qlineargradient(
        x1: 0, y1: 1, x2: 1, y2: 0,
        stop: 0 rgb(255, 130, 136),
  
        stop: 1 rgb(244, 212, 212)
    );


    color: white;               /* White text */
    border: none;
    border-radius: 10px;
   
    font: 500 12px &quot;Inter&quot;;
   

    transition: background-color 0.3s ease;
}

/* Hover state */
QToolButton:hover {
    background-color: rgb(230, 146, 149)
}

/* Pressed state */
QToolButton:pressed {
    background-color: rgb(144, 72, 101) pressed */
}

/* Disabled state */
QToolButton:disabled {
    background-color: #A6A6A6;  /* Gray background when disabled */
    color: #666666;
    cursor: default;
}

/* Optional: Different style for Sign Up button */
QToolButton#signUpButton {
    background-color: #28A745;  /* Green */
}

QToolButton#signUpButton:hover {
    background-color: #1E7E34;
}

QToolButton#signUpButton:pressed {
    background-color: #155D27;
}
